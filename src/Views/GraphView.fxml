<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.GraphViewController">
   <stylesheets>
       <URL value="@MobilePhoneStyles.css"/>
   </stylesheets>
    <children>
      <VBox alignment="TOP_CENTER" layoutX="59.0">
         <children>
            <BarChart fx:id="barChart">
              <xAxis>
                <CategoryAxis fx:id="movies" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="students" side="LEFT" />
              </yAxis>
            </BarChart>
            <Button mnemonicParsing="false" onAction="#returnToTableViewButton" text="Back to Table View" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
